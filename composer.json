{
    "name": "w3suga/w3cms",
    "description": "The CMS by Laravel Framework.",
    "keywords": ["framework", "laravel", "cms"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "^7.1.3",
        "fideloper/proxy": "^4.0",
        "laravel/framework": "5.6.*",
        "laravel/tinker": "^1.0",
        "tormjens/eventy": "^0.5.4",
        "salipropham/laravel-mvcsp": "^0.1.0",
        "spatie/laravel-permission": "^2.36",
        "symfony/dom-crawler": "^4.2",
        "intervention/image": "^2.4",
        "doctrine/dbal": "^2.9",
        "akaunting/setting": "^1.0",
        "nwidart/laravel-modules": "^5.0",
        "spatie/laravel-translatable": "3.1.3"
    },
    "require-dev": {
        "filp/whoops": "^2.0",
        "fzaninotto/faker": "^1.4",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^2.0",
        "phpunit/phpunit": "^7.0"
    },

    "autoload": {
        "files": [
            "app/helpers.php",
            "app/cms_helpers.php"
        ],
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/",
            "Modules\\": "Modules/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
            ]
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ]
    },
    "config": {
        "allow-plugins": {
            "kylekatarnls/update-helper": true
        }
    }
}
